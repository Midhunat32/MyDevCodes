<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.wisilica.androdev">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".ui.activity.AnimationsSample.ThinkOut">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>

        </activity>
        <activity android:name=".ui.activity.AnimationsSample.FallDownAnimationActivity">
        </activity>
        <activity android:name=".ui.activity.AnimationsSample.AnimationPath">
        </activity>
        <activity android:name=".ui.activity.AnimationsSample.AnimationActivity">
        </activity>

        <meta-data
                android:name="com.google.firebase.messaging.default_notification_color"
                android:resource="@color/colorAccent"/>

        <service
                android:name=".ui.activity.Firebase.MessageServices"
                android:enabled="true"
                android:exported="true">
        </service>

        <activity android:name=".ui.activity.HomeActivity">
        </activity>
        <activity android:name=".ui.activity.TransitionActivity">
        </activity>
        <activity android:name=".ui.activity.dqlite_db.DataBaseActivity">
        </activity>
        <activity android:name=".ui.activity.JavaTest.Animal">
        </activity>
        <activity
                android:name=".ui.activity.room.RoomActivity"
                android:windowSoftInputMode="adjustPan">
        </activity>
        <activity android:name=".ui.activity.Firebase.FireBaseActivity">
        </activity>
        <activity android:name=".ui.activity.FbAuthentication.FbLogin">
        </activity>

        <meta-data
                android:name="com.facebook.sdk.ApplicationId"
                android:value="@string/facebookapp_id">
        </meta-data>
    </application>

</manifest>